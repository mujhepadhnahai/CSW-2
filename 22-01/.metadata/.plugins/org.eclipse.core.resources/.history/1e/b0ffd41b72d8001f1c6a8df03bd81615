package oops;

public class Student {
         int roll;
         String name;
         
         Student(int i, String s) {
        	 roll = i;
        	 name = s;
		}
         void print() {
        	 System.out.print("Student details:");
        	 System.out.print("Name :"+name);
        	 System.out.print("Roll :"+roll);
         }
}
